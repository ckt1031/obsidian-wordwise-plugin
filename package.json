{
	"name": "obsidian-ai-plugin",
	"version": "0.0.2",
	"description": "Obsidian plugin for AI content generation",
	"license": "MIT",
	"author": "ckt1031",
	"main": "main.js",
	"scripts": {
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"dev": "node esbuild.config.mjs",
		"format": "prettier --write .",
		"lint": "tsc -noEmit -skipLibCheck && eslint . --ext .ts,.js,.mjs",
		"lint:fix": "eslint . --ext .ts,.js,.mjs --fix",
		"version": "node version-bump.mjs && git add manifest.json versions.json"
	},
	"devDependencies": {
		"@ckt1031/eslint-config": "1.1.9",
		"@ckt1031/prettier-config": "1.1.9",
		"@types/mustache": "4.2.2",
		"@types/node": "20.3.1",
		"@typescript-eslint/eslint-plugin": "5.60.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.18.8",
		"eslint": "8.43.0",
		"obsidian": "1.2.8",
		"prettier": "2.8.8",
		"tslib": "2.5.3",
		"typescript": "5.1.3"
	},
	"packageManager": "pnpm@8.6.3",
	"prettier": "@ckt1031/prettier-config"
}
